{
  "extends": [
    "react-app"
  ],
  "rules": {
    "semi": ["error", "always"],
    "keyword-spacing": ["error", { "before": true }],
    "space-before-blocks": "error",
    "no-trailing-spaces": "error",
    "no-multi-spaces": "error"
  },
  "overrides": [
    {
      "files": ["**/*.ts?(x)"],
      "rules": {}
    }
  ]
}
